{
  "name": "react-context-helper",
  "version": "1.5.3",
  "description": "Tiny library that provides easy updating of context and optimization of context consumers.",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/react-context-helper.umd.cjs",
  "module": "./dist/react-context-helper.js",
  "exports": {
    ".": {
      "import": "./dist/react-context-helper.js",
      "require": "./dist/react-context-helper.umd.cjs"
    }
  },
  "scripts": {
    "test": "jest --bail --passWithNoTests --coverage --collectCoverageFrom=lib/components/**/*.jsx",
    "preview": "vite preview",
    "build": "vite build",
    "dev": "vite",
    "prepare": "husky install",
    "lint": "eslint --cache --fix"
  },
  "homepage": "https://github.com/theCocoonStudio/react-context-helper",
  "keywords": [
    "React",
    "optimization",
    "javascript",
    "typescript",
    "react component",
    "react context",
    "context",
    "react optimization"
  ],
  "author": "Izzy Erlich",
  "license": "MIT",
  "browserslist": [
    "defaults",
    "maintained node versions"
  ],
  "lint-staged": {
    "*.jsx": [
      "eslint --cache --fix",
      "npm run test"
    ],
    "*.js": [
      "eslint --cache --fix",
      "npm run test"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^2.2.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-jest": "^27.4.6",
    "enzyme": "^3.11.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-syntax-highlighter": "^15.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.56.2",
    "vite": "^3.2.4",
    "vite-plugin-external": "^1.2.7"
  },
  "dependencies": {
    "immer": "^9.0.12",
    "use-immer": "^0.8.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
