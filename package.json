{
  "name": "react-context-helper",
  "version": "1.2.3",
  "description": "Tiny library that provides easy updating of context and optimization of context consumers.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-context-helper.umd.cjs",
  "module": "./dist/react-context-helper.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-context-helper.es.js",
      "require": "./dist/react-context-helper.umd.cjs"
    }
  },
  "scripts": {
    "test": "jest --bail --passWithNoTests --coverage --collectCoverageFrom=lib/components/**/*.jsx",
    "preview": "vite preview",
    "build": "vite build",
    "dev": "vite",
    "prepare": "husky install",
    "lint": "eslint --cache --fix",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "homepage": "https://github.com/theCocoonStudio/react-context-helper",
  "keywords": [
    "React",
    "optimization",
    "javascript",
    "typescript",
    "react component",
    "react context",
    "context",
    "react optimization"
  ],
  "author": "Izzy Erlich",
  "license": "MIT",
  "browserslist": [
    "defaults",
    "maintained node versions"
  ],
  "lint-staged": {
    "*.jsx": [
      "eslint --cache --fix",
      "npm run test"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
